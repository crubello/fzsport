//

[
  {
    $group: {
      _id: "$User ID",
      torneos_vistos: {
        $push: "$Torneo",
      },
      equipos_vistos: {
        $push: "$Equipos",
      },
    },
  },

  {
    $merge: {
      into: "users_vistos",
      on: "_id",
      whenMatched: "merge",
      whenNotMatched: "discard",
    },
  },
]
